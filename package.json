{
	"name": "blog-template",
	"type": "module",
	"version": "1.1.0",
	"description": "Fintech Guide Website",
	"license": "GPL-3.0-only",
	"keywords": [
		"astro",
		"astrojs",
		"astro-template",
		"astro-blog",
		"astro-b"
	],
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"sync": "astro sync",
		"preview": "astro preview",
